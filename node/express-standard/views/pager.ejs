<div id='pager'>
    <% if(page.count > 1) { %>
        <% if(page.number > 1) {%>
            <a id="prev" href="/entry/list?page=<%= page.number - 1 %>">Prev<a>
        <% } %>
        <% if(page.number < page.count ) { %>
            <% if(page.number) { %>
                &nbsp;&nbsp;
            <% } %>
            <a id="next" href="/entry/list?page=<%= page.number + 1 %>">Next<a>
        <% } %>
    <% } %>
</div>